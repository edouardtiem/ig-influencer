{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "functions": {
    "src/app/api/auto-post/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-contextual/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/daily-trends/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/smart-comment/route.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "elenav.link"
        }
      ],
      "destination": "/elena"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.elenav.link"
        }
      ],
      "destination": "/elena"
    }
  ]
}
